{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [
    "plugins/markdown",
    "/usr/local/lib/node_modules/tsdoc/template/plugins/TSDoc.js"
  ],
  "opts": {
    "template": "/usr/local/lib/node_modules/tsdoc/template",
    "recurse": "true"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "source": {
    "includePattern": "(\\.d)?\\.ts$"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "tsdoc": {
    "source": "./src/js",
    "destination": "./build/docs",
    "tutorials": "docs",
    "systemName": "edia-frontend-build-proposal",
    "footer": "",
    "copyright": "edia-frontend-build-proposal Copyright Â© 2016 rik.",
    "outputSourceFiles": true,
    "commentsOnly": true
  }
}

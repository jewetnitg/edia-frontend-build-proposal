{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "build.config.js",
      "gulpfile.js",
      "index.js",
      "karma.conf.js",
      "src",
      "tasks"
    ]
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName": "edia-frontend-build-proposal",
    "footer": "",
    "copyright": "EDIA 2016",
    "navType": "vertical",
    "theme": "paper",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "./build/docs",
    "template": "./node_modules/ink-docstrap/template",
    "recurse": true,
    "readme": "./README.md",
    "tutorials": "./docs"
  }
}